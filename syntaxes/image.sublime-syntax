%YAML 1.2
---
# http://www.sublimetext.com/docs/3/syntax.html
# Used for image formats that ST doesn't support natively
name: Image
file_extensions:
  - emf
  - jp2
  - pnm
  - ppm
  - tga
  - tif
  - tiff
  - webp
  - xbm
  - xpm
scope: text.plain.image
hidden: true
contexts:
  main:
    - include: scope:text.plain#prototype
    - include: scope:text.plain
